<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="OrganicProductBase">
    <defName>Green_Milk</defName>
    <label>Green Milk</label>
    <description>Strange green pus from a gecko's tail. It tastes surprisingly good, and is quite nutritious.</description>
    <graphicData>
      <texPath>Things/Items/Resources/MilkTypes/Green_Milk</texPath>
    </graphicData>
    <socialPropernessMatters>true</socialPropernessMatters>
    <statBases>
      <MarketValue>1.8</MarketValue>
      <Mass>0.045</Mass>
      <MaxHitPoints>60</MaxHitPoints>
      <Flammability>0.2</Flammability>
      <Nutrition>0.06</Nutrition>
      <FoodPoisonChanceFixedHuman>0.03</FoodPoisonChanceFixedHuman>
    </statBases>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>20</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
    <ingestible>
      <preferability>RawTasty</preferability>
      <foodType>AnimalProduct, Fluid</foodType>
    </ingestible>
    <thingCategories>
      <li>ST_Milk</li>
    </thingCategories>
  </ThingDef>

