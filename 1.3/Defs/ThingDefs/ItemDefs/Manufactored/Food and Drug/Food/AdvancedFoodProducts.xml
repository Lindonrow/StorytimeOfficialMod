<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<ThingDef ParentName="MealBase">
    <defName>MeatNugget</defName>
    <label>Meat Nugget</label>
    <description>A nugget made of meat.</description>
    <graphicData>
      <texPath>Things/Items/Resources/MeatNugget</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <statBases>
      <DeteriorationRate>0.25</DeteriorationRate>
      <MarketValue>24</MarketValue>
      <Mass>0.3</Mass>
      <WorkToMake>450</WorkToMake>
      <Nutrition>0.9</Nutrition>
    </statBases>
    <ingestible>
      <preferability>MealSimple</preferability>
      <ingestEffect>EatVegetarian</ingestEffect>
      <ingestSound>Meal_Eat</ingestSound>
	  <outcomeDoers>
        <li Class="IngestionOutcomeDoer_GiveHediff">
          <hediffDef>HedNuggie</hediffDef>
          <severity>1</severity>
        </li>
      </outcomeDoers>
    </ingestible>
	<comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>0.75</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
  </ThingDef>
  
  
