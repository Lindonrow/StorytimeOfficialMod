<?xml version="1.0" encoding="utf-8" ?>
<Defs>

 <ThingDef ParentName="PlantBase">
    <defName>Plant_Botox</defName>
    <label>Botox Plant</label>
    <description>A strange plant that grows little flesh colored fruit with high concentrations of botox.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Nutrition>0.1</Nutrition>
    </statBases>
    <graphicData>
      <texPath>Things/Plants/Botox</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <selectable>true</selectable>
    <pathCost>14</pathCost>
    <ingestible/>
    <plant>
      <growDays>10</growDays>
      <dieIfLeafless>true</dieIfLeafless>
      <harvestTag>Standard</harvestTag>
      <harvestedThingDef>BotoxFruit</harvestedThingDef>
      <harvestYield>10</harvestYield>
      <sowTags>
        <li>Ground</li>
        <li>Hydroponic</li>
      </sowTags>
      <topWindExposure>0.1</topWindExposure>
      <immatureGraphicPath>Things/Plants/BotoxImmature</immatureGraphicPath>
      <visualSizeRange>0.4~1.2</visualSizeRange>
      <wildOrder>2</wildOrder>
    </plant>
  </ThingDef>
  
</Defs>
