<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <RecipeDef>
    <defName>Make_CheesePizza</defName>
    <label>Make a cheese pizza</label>
    <description>Make a cheese pizza.</description>
    <jobString>Making.</jobString>
    <workAmount>600</workAmount>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <ingredients>
      <li>
        <filter>
					<thingDefs>
						<li>Cheese</li>
					</thingDefs>
        </filter>
        <count>5</count>
      </li>
	  <li>
        <filter>
					<thingDefs>
						<li>TomatoCyst</li>
					</thingDefs>
        </filter>
        <count>5</count>
      </li>
	  <li>
        <filter>
          <categories>
            <li>PlantFoodRaw</li>
          </categories>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
					<thingDefs>
						<li>TomatoCyst</li>
						<li>Cheese</li>
					</thingDefs>
					<categories>
						<li>PlantFoodRaw</li>
					</categories>
    </fixedIngredientFilter>
    <defaultIngredientFilter>
					<thingDefs>
						<li>TomatoCyst</li>
						<li>Cheese</li>
					</thingDefs>
					<categories>
						<li>MeatRaw</li>
						<li>PlantFoodRaw</li>
					</categories>
    </defaultIngredientFilter>
	<recipeUsers>
			<li>ElectricStove</li>
			<li>FueledStove</li>
	</recipeUsers>
	<researchPrerequisite>pizzaTime</researchPrerequisite>
	<products>
		<CheesePizza>1</CheesePizza>
	</products>
  </RecipeDef>
  
