<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<RecipeDef>
    <defName>Make_PadCandy</defName>
    <label>make d-pad candy</label>
    <description>Grind plants into candy, shape it like a D-Pad for some reason.</description>
    <jobString>Making.</jobString>
    <workAmount>400</workAmount>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <ingredients>
      <li>
        <filter>
					<thingDefs>
						<li>Sugar</li>
					</thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
		<thingDefs>
			<li>Sugar</li>
		</thingDefs>
    </fixedIngredientFilter>
    <defaultIngredientFilter>
		<thingDefs>
			<li>Sugar</li>
		</thingDefs>
    </defaultIngredientFilter>
	<recipeUsers>
    		<li>ElectricStove</li>
		    <li>FueledStove</li>
	</recipeUsers>
	<researchPrerequisite>advancedCooking</researchPrerequisite>
	<products>
		<PadCandy>1</PadCandy>
	</products>
  </RecipeDef>

