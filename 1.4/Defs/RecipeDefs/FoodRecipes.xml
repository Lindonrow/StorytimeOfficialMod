				</thingDefs>
    </defaultIngredientFilter>
	<recipeUsers>
    		<li>ElectricStove</li>
		    <li>FueledStove</li>
	</recipeUsers>
	<researchPrerequisite>advancedCooking</researchPrerequisite>
	<products>
		<ClamSauce>1</ClamSauce>
	</products>
  </RecipeDef>
  
