  <RecipeDef>
    <defName>Make_Burrito</defName>
    <label>Make a Beefy Five Layer Burrito</label>
    <description>Make a Beefy Five Layer Burrito.</description>
    <jobString>Making.</jobString>
    <workAmount>400</workAmount>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <ingredients>
      <li>
        <filter>
					<thingDefs>
						<li>Bean</li>
					</thingDefs>
        </filter>
        <count>5</count>
      </li>
	  <li>
        <filter>
					<thingDefs>
						<li>Cheese</li>
					</thingDefs>
        </filter>
        <count>5</count>
      </li>
      <li>
        <filter>
          <categories>
            <li>MeatRaw</li>
          </categories>
        </filter>
        <count>5</count>
      </li>
	  <li>
        <filter>
          <categories>
            <li>PlantFoodRaw</li>
