  <RecipeDef>
    <defName>Make_ClamSauce</defName>
    <label>make clam sauce</label>
    <description>Make sauce from clam and potato.</description>
    <jobString>Making.</jobString>
    <workAmount>300</workAmount>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <ingredients>
      <li>
        <filter>
					<thingDefs>
						<li>ST_Clam</li>
					</thingDefs>
        </filter>
        <count>5</count>
      </li>
	  <li>
        <filter>
					<thingDefs>
						<li>RawPotatoes</li>
					</thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
				<thingDefs>
					<li>RawPotatoes</li>
					<li>Cheese</li>
				</thingDefs>
    </fixedIngredientFilter>
    <defaultIngredientFilter>
				<thingDefs>
					<li>RawPotatoes</li>
