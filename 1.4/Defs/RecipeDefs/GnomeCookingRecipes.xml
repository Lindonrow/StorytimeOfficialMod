    <defName>Make_GnomeFlour</defName>
    <label>Grind gnomeroots</label>
    <description>Grind gnomeroot.</description>
    <jobString>Grinding.</jobString>
    <workAmount>600</workAmount>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <ingredients>
      <li>
        <filter>
					<thingDefs>
