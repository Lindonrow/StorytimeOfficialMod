<Defs>

  <ThingDef ParentName="PlantBase">
    <defName>Herb_Guam</defName>
    <label>guam bush</label>
    <description>A bitter green herb.</description>
    <statBases>
      <MaxHitPoints>120</MaxHitPoints>
      <Nutrition>0.50</Nutrition>
    </statBases>
    <graphicData>
      <texPath>Things/Plants/Herbs/Guam</texPath>
      <graphicClass>Graphic_Random</graphicClass>
      <shadowData>
        <volume>(0.3, 0.1, 0.1)</volume>
      </shadowData>
    </graphicData>
    <selectable>true</selectable>
    <neverMultiSelect>false</neverMultiSelect>
    <fillPercent>0.20</fillPercent>
    <pathCost>30</pathCost>
    <plant>
      <fertilityMin>0.5</fertilityMin>
      <fertilitySensitivity>0.5</fertilitySensitivity>
      <growDays>8</growDays>
      <harvestYield>12</harvestYield>
      <harvestWork>250</harvestWork>
      <harvestTag>Standard</harvestTag>
      <harvestAfterGrowth>0.30</harvestAfterGrowth>
      <harvestedThingDef>GuamLeaves</harvestedThingDef>
      <wildClusterRadius>2</wildClusterRadius>
      <wildClusterWeight>250</wildClusterWeight>
      <visualSizeRange>0.95~1.7</visualSizeRange>
      <wildOrder>2</wildOrder>
		<wildBiomes>
		  <TemperateForest>0.01</TemperateForest>
		  <TemperateSwamp>0.005</TemperateSwamp>
		  <BorealForest>0.005</BorealForest>
		  <ColdBog>0.005</ColdBog>
		  <AridShrubland>0.005</AridShrubland>
		</wildBiomes>
    </plant>
  </ThingDef>
  
