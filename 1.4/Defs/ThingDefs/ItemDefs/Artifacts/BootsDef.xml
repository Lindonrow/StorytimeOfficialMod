<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <ThingDef ParentName="ApparelMakeableBase">
    <defName>Cartographer_Boots</defName>
    <label>Boots of the Cartographer</label>
    <description>These boots harness the power of the cartographer herself...</description>
	<generateCommonality>0.01</generateCommonality>
    <recipeMaker>
      <researchPrerequisite>ComplexClothing</researchPrerequisite>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Items/Cartographer_Shoes</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <costList>
      <Leather_Cartographer>40</Leather_Cartographer>
    </costList>
    <apparel>
	  <careIfWornByCorpse>true</careIfWornByCorpse>
      <bodyPartGroups>
        <li>Feet</li>
      </bodyPartGroups>
      <layers>
        <li>Middle</li>
      </layers>
      <defaultOutfitTags>
        <li>Worker</li>
      </defaultOutfitTags>
      <tags>
        <li>Outlander</li>
      </tags>
    </apparel>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <Mass>0.25</Mass>
      <WorkToMake>3000</WorkToMake>
      <EquipDelay>0.8</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <MoveSpeed>+0.7</MoveSpeed>
    </equippedStatOffsets>
	<thingCategories>
      <li>Apparel</li>
    </thingCategories>
  </ThingDef>

</Defs>