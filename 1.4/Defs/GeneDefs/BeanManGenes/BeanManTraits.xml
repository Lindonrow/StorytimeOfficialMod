    <displayOrderInCategory>1999</displayOrderInCategory>
	<statOffsets>	
			  <SocialImpact>-0.2</SocialImpact>
	</statOffsets>		
    <exclusionTags>
      <li>BeanFriendliness</li>
    </exclusionTags>
  </GeneDef>
  
