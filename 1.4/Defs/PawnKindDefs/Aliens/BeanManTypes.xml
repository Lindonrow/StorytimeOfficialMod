<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <PawnKindDef Abstract="True" Name="BasePlayerBeanManPawnKind">
    <race>Alien_BeanMan</race>
    <isFighter>false</isFighter>
    <apparelIgnoreSeasons>true</apparelIgnoreSeasons>
    <forceNormalGearQuality>false</forceNormalGearQuality>
    <invNutrition>2.55</invNutrition>
  </PawnKindDef>
  
  <!-- Player -->
  <PawnKindDef ParentName="BasePlayerBeanManPawnKind">
    <defName>PlayerBeanMan</defName>
    <label>member</label>
    <combatPower>30</combatPower>
    <defaultFactionType>PlayerColony</defaultFactionType>
    <backstoryCryptosleepCommonality>0</backstoryCryptosleepCommonality>
	<backstoryFiltersOverride>
	  <li>
		<categories>
		  <li>BeanManBackstory</li>
		</categories>
	  </li>
	</backstoryFiltersOverride>
	<maxGenerationAge>10</maxGenerationAge>
    <initialWillRange>1~4</initialWillRange>
    <initialResistanceRange>5~10</initialResistanceRange>
    <xenotypeSet>
      <xenotypeChances>
        <RedBeanMan>35</RedBeanMan>
        <OrangeBeanMan>7</OrangeBeanMan>
        <BlueBeanMan>10</BlueBeanMan>
        <BlackBeanMan>5</BlackBeanMan>
        <BottBeanMan>3</BottBeanMan>
        <CaveBeanMan>3</CaveBeanMan>
        <ChiliBeanMan>5</ChiliBeanMan>
        <CoffeeBeanMan>5</CoffeeBeanMan>
        <GreenBeanMan>7</GreenBeanMan>
        <PurpleBeanMan>5</PurpleBeanMan>
        <ShinyBeanMan>1</ShinyBeanMan>
        <SilverBeanMan>5</SilverBeanMan>
        <StinkyBeanMan>2</StinkyBeanMan>
        <WhiptongueBeanMan>7</WhiptongueBeanMan>
		<PolarBeanMan>3</PolarBeanMan>
      </xenotypeChances>
    </xenotypeSet>
  </PawnKindDef>
  
