			<rulesStrings>
				<li>name->[MaleList] [FamilyList]</li>
			</rulesStrings>
			<rulesRaw>
				<li Class="Rule_File">
					<keyword>MaleList</keyword>
					<path>ElfNames/ElfMaleNames</path>
