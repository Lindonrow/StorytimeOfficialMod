<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RulePackDef>
		<defName>GnomeMaleName</defName>
		<rulePack>
			<rulesStrings>
				<li>name->[MaleList] [FamilyList]</li>
			</rulesStrings>
			<rulesRaw>
				<li Class="Rule_File">
					<keyword>MaleList</keyword>
					<path>GnomeNames/GnomeMale</path>
				</li>
				<li Class="Rule_File">
					<keyword>FamilyList</keyword>
					<path>GnomeNames/GnomeLastNames</path>
				</li>
			</rulesRaw>
		</rulePack>
	</RulePackDef>

	<RulePackDef>
		<defName>GnomeFemaleName</defName>
		<rulePack>
			<rulesStrings>
				<li>name->[FemaleList] [FamilyList]</li>
			</rulesStrings>
			<rulesRaw>
				<li Class="Rule_File">
					<keyword>FemaleList</keyword>
					<path>GnomeNames/GnomeFemale</path>
				</li>
				<li Class="Rule_File">
					<keyword>FamilyList</keyword>
					<path>GnomeNames/GnomeLastNames</path>
				</li>
			</rulesRaw>
		</rulePack>
	</RulePackDef>
	
	<RulePackDef>
    <defName>GnomeSettlementName</defName>
    <rulePack>
      <rulesStrings>
        <li>name-&gt;[pack]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>pack</keyword>
          <path>GnomeNames/GnomeSettlement</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
	
	
</Defs>