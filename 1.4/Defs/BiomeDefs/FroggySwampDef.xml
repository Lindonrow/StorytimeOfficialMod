<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <BiomeDef>
    <defName>froggySwamp</defName>
    <label>froggy swamp</label>
    <description>A region of swamplands that, due to its particularly muddy environment, has come to be home solely to a variety of frog species.</description>
    <workerClass>BiomesKit.UniversalBiomeWorker</workerClass>
	<modExtensions>
		  <li Class="BiomesKit.BiomesKitControls">
		  <spawnOnBiomes>
			<li>TropicalSwamp</li>
			<li>TemperateSwamp</li>
		  </spawnOnBiomes>
		  <maxHilliness>1</maxHilliness>
		  <frequency>50</frequency>
		  <usePerlin>true</usePerlin>
		  <perlinCulling>0.25</perlinCulling>
		  </li>
		<li Class="RockSettings.DefModExtension_BiomeSettings">
			<rockCount>1~1</rockCount>
			<allowedRockDefs>
				<li>mudstone</li>
			</allowedRockDefs>
