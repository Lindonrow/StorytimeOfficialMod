<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Genetics Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/RecipeDef[defName = "GR_ExtractGenesCanine"]/ingredients/li/filter/thingDefs</xpath>
					<value>
						<li>Corpse_RatDog</li>
						<li>Corpse_Dane</li>
						<li>Corpse_Jonah_Dog</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/RecipeDef[defName = "GR_ExtractGenesCanine"]/fixedIngredientFilter/thingDefs</xpath>
					<value>
						<li>Corpse_RatDog</li>
						<li>Corpse_Dane</li>
						<li>Corpse_Jonah_Dog</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						<GeneticRim.ExtractableAnimalsList>
							<defName>GR_TierOneAnimalsCanineCatsDogs</defName>
							<tier>1</tier>
							<itemProduced>GR_CanineGenetic</itemProduced>
							<extractableAnimals>
								<li>RatDog</li>
								<li>Dane</li>
								<li>Jonah_Dog</li>
							</extractableAnimals>
						</GeneticRim.ExtractableAnimalsList>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>